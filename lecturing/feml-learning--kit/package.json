{
  "name": "feml-project-kit",
  "version": "1.0.0",
  "description": "Front-End Masters League 프로젝트 킷",
  "scripts": {
    "start": "npm run dev:open",
    "dev": "parcel src/index.html --port 8080",
    "dev:open": "npm run dev -- --open",
    "build": "parcel build src/index.html"
  },
  "postcss": {
    "map": {
      "inline": true,
      "sourcesContent": false
    },
    "plugins": {
      "autoprefixer": {
        "grid": true
      }
    }
  },
  "stylus": {
    "sourcemap": {
      "comment": false,
      "inline": false,
      "basePath": "./src/sourcemap"
    }
  },
  "keywords": [
    "parcel",
    "babel",
    "stylus",
    "postcss",
    "gsap",
    "flexibility"
  ],
  "author": "yamoo9 <yamoo9@naver.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.1",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.4.1",
    "stylus": "^0.54.7"
  },
  "dependencies": {
    "gsap": "^3.0.1"
  }
}
