  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.2",
    "json-server": "^0.15.1",
    "live-server": "^1.2.1",
    "node-cmd": "^3.0.0",
    "parcel-bundler": "^1.12.4",
    "path": "^0.12.7",
    "postcss-modules": "^1.4.1",
    "shx": "^0.3.2",
    "stylus": "^0.54.7",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "gsap": "^3.0.1",
    "jsx-vanilla": "^3.1.1"
