{
  "name": "feml-project-kit",
  "version": "1.0.1",
  "description": "Front-End Masters League 프로젝트 킷",
  "scripts": {
    "start": "npm run clean && npm run dev",
    "clean": "shx rm -rf ./.cache ./dist ./src/css",
